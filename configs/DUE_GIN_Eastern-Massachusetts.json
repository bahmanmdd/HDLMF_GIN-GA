{
    "params_opt": {
        "seed": 7,
        "epochs": 1000,
        "batch_size": 64,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.9,
        "lr_schedule_patience": 10,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 1,
        "max_time": 50
